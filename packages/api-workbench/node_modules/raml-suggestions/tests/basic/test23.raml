#%RAML 1.0
title: RAML
mediaType: [multipart/form-data, application/json]
securedBy: [ oauth_2_0, oauth_1_0 ]
securitySchemes:
  oauth_2_0:
    type: OAuth 2.0
  oauth_1_0:
    type: OAuth 1.0
/node:
  description: A resource
  securedBy: oauth_1_0
  post:
    description: A method
    displayName:
/res:
  securedBy: [oauth_2_0, oauth_1_0]
  post:
    securedBy: oauth_2_0
  /chld:
    patch:
      securedBy: [oauth_2_0, oauth_1_0]
